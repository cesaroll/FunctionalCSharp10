@using Models
<table class="table table-striped">
    @foreach (Year year in DateTime.UtcNow.ToYear().Decade)
    {
        <tr>
            <th>@year.Number</th>
            @foreach(Month month in year.Months)
            {
                <td>@($"{this.MonthNames[month.Ordinal]}")</td>
            }
    </tr>
    }
</table>
@code {
    [Parameter]
    public DateTime ForTimestamp {get; set;}

    string[] MonthNames = new[] {
        "", "ENE", "FEB", "MAR", "ABR", "MAY", "JUN",
        "JUL", "AGO", "SEP", "OCT", "NOV", "DEC"
    };
}
